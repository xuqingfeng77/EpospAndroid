<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 默认时的背景图片-->
    <item android:state_pressed="false" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners
                android:topLeftRadius="5sp"
                android:topRightRadius="5sp"
                android:bottomLeftRadius="5sp"
                android:bottomRightRadius="5sp" />
            <solid
                android:color="@color/white"/><!-- 填充的颜色 -->
            <stroke android:color="@color/left_text_color"
                android:width="1dp"></stroke>
        </shape>
    </item>
    <!--按下时的图片背景-->
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners
                android:topLeftRadius="5sp"
                android:topRightRadius="5sp"
                android:bottomLeftRadius="5sp"
                android:bottomRightRadius="5sp" />
            <solid
                android:color="@color/white"/><!-- 填充的颜色 -->
            <stroke android:color="#99999999"
                android:width="1dp"></stroke>
        </shape>
    </item>
</selector>